{
  "name": "postgres_Sink",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
    "tasks.max": "1",
    "topics": "CDC.public.test",
    "connection.url": "jdbc:postgres://",
    "connection.user": "MYUSER",
    "connection.password": "password",
    "insert.mode": "upsert",
    "delete.enabled": "true",
    "table.name.format": "cdc-postgres-table",
    "pk.mode": "record_key",
    "pk.fields": "id",
    "auto.create": "true",
    "auto.evolve": "true"
  }
}

